{
  "appearance": {
    "mode": "dark",
    "theme": "blue"
  },
  "connection": {
    "default_baudrate": 38400,
    "timeout": 30,
    "auto_connect": false,
    "preferred_port": "",
    "type": "serial"
  },
  "bluetooth": {
    "scan_timeout": 10,
    "connect_timeout": 10,
    "last_device": null
  },
  "monitoring": {
    "update_interval": 0.5,
    "max_log_entries": 1000,
    "auto_save_logs": true,
    "log_directory": "logs"
  },
  "alerts": {
    "enable_audio": true,
    "enable_visual": true,
    "dtc_alerts": true,
    "threshold_alerts": true
  },
